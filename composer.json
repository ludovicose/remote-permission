{
    "name": "ludovicose/remote-permission",
    "description": "Check user permission in remote server",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "framework",
        "laravel"
    ],
    "homepage": "https://github.com/ludovicose/remote-permission",
    "authors": [
        {
            "name": "Aibek Muratbekov",
            "email": "aibek.muratbekov@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": ">=6.3"
    },
    "require-dev": {
        "vimeo/psalm": "^4.18.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Ludovicose\\RemotePermission\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ludovicose\\RemotePermission\\Providers\\RemotePermissionServiceProvider"
            ]
        }
    }
}
