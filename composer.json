{
    "name": "ludovicose/remote-permission",
    "description": "Check user permission in remote server",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Spark\\RemotePermission\\": "src"
        }

    },
    "extra": {
        "laravel": {
            "providers": [
                "Spark\\RemotePermission\\RemotePermissionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
